<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_saleorder_document_multi_branch" inherit_id="sale.report_saleorder_document">
        <xpath expr="//div[@id='informations']/div[6]" position="after">
            <div t-if="doc.branch_id" class="col-auto mw-100 mb-2">
                <strong>Branch:</strong>
                <p class="m-0" t-field="doc.branch_id"/>
            </div>
        </xpath>
    </template>
</odoo>
