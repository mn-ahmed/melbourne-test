<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="consignment_transfer_sequence" model="ir.sequence">
            <field name="name">Consignment Transfer Sequence</field>
            <field name="implementation">no_gap</field>
            <field name="code">consignment.transfer</field>
            <field name="prefix">CT-</field>
            <field name="padding">6</field>
            <field name="number_next_actual" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
        <record id="consignment_return_sequence" model="ir.sequence">
            <field name="name">Consignment Return Sequence</field>
            <field name="implementation">no_gap</field>
            <field name="code">consignment.return</field>
            <field name="prefix">CR-</field>
            <field name="padding">6</field>
            <field name="number_next_actual" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="active" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>